{
  "env": {
    "SKIP_ENV_VALIDATION": "true",
    "PRISMA_ENGINES_CHECKSUM_IGNORE_MISSING": "1"
  },
  "buildCommand": "npx prisma generate && npm run build",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}